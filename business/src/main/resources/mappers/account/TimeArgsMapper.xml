<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soft.one.ewms.business.account.mapper.TimeArgsMapper">
  <resultMap id="BaseResultMap" type="com.soft.one.ewms.domain.TimeArgs">
    <!--@mbg.generated-->
    <!--@Table time_args-->
    <id column="User_id" jdbcType="VARCHAR" property="userId" />
    <id column="Args_date" jdbcType="TIMESTAMP" property="argsDate" />
    <result column="Stay_time" jdbcType="INTEGER" property="stayTime" />
    <result column="Remind_time" jdbcType="INTEGER" property="remindTime" />
  </resultMap>
  <sql id="Base_Column_List">
    <!--@mbg.generated-->
    User_id, Args_date, Stay_time, Remind_time
  </sql>

  <update id="updateTimeArgs">
    UPDATE  time_args
        SET
        Args_date = #{argsDate},
        User_id = #{userId},
        Stay_time = #{stayTime},
        Remind_time = #{remindTime}
        WHERE Args_date = #{argsDate}
  </update>

  <select id="selectTimeArgs" resultType="TimeArgs">
    select * from time_args
  </select>
</mapper>